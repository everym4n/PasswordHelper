<?xml version="1.0" encoding="utf-8"?>
<clip xmlns:android="http://schemas.android.com/apk/res/android" >

    <level-list>
        <item android:minLevel="0" android:maxLevel="3999">
            <shape>
                <solid android:color="@color/color_bad_password"/>
            </shape>


        </item>
        <item android:minLevel="4000" android:maxLevel="7999">
            <shape>
                <solid android:color="@color/color_medium_password"/>
            </shape>
        </item>
        <item android:minLevel="8000" android:maxLevel="10000">
            <shape>
                <solid android:color="@color/color_good_password"/>
            </shape>
        </item>
    </level-list>
</clip>